    {
      "name": "Start jaime",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}/jaime",
      "windows": {
        "runtimeExecutable": "npm.cmd",
        "args": [
          "--",
          "--ui5Uri https://ui5.sap.com --ui5 1.88.1",
          "--backendConfig",
          "\"[{\\\"path\\\":\\\"/sap\\\",\\\"destination\\\":\\\"Northwind\\\"}]\""
        ]
      },
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {
        "DEBUG": "--inspect",
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/jaime\"}"
      },
      "args": [
        "--",
        "--ui5Uri https://ui5.sap.com --ui5 1.88.1",
        "--backendConfig",
        "[{\"path\":\"/sap\",\"destination\":\"Northwind\"}]"
      ]
    },
    {
      "name": "Start jaime Mock",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/jaime",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "runtimeArgs": [
        "run-script",
        "start-mock"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/jaime\"}"
