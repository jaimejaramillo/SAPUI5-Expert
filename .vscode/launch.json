{
  "inputs": [
    {
      "id": "UI5Version",
      "type": "command",
      "command": "sap.ux.appGenerator.ui5Version"
    }
  ],
  "configurations": [
    {
      "name": "Run Forms-Front",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Forms/forms-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsFront/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Forms/Front",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Forms/Front\"}"
      },
      "envFile": "${workspaceFolder}/Forms/Front/.env2",
      "preLaunchTask": "Build Forms"
    },
    {
      "name": "Run Invoices-SAPUI5",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Invoices/invoices-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsSAPUI5/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Invoices/SAPUI5",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Invoices/SAPUI5\"}"
      },
      "envFile": "${workspaceFolder}/Invoices/SAPUI5/.env6",
      "preLaunchTask": "Build Invoices"
    },
    {
      "name": "Run MockData Invoices-SAPUI5",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Invoices/invoices-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsSAPUI5/test/mockServer.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Invoices/SAPUI5",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Invoices/SAPUI5\"}"
      },
      "envFile": "${workspaceFolder}/Invoices/SAPUI5/.env7",
      "preLaunchTask": "Build Invoices"
    },
    {
      "name": "Run Test Invoices-SAPUI5",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Invoices/invoices-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsSAPUI5/test/unit/unitTest.qunit.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Invoices/SAPUI5",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Invoices/SAPUI5\"}"
      },
      "envFile": "${workspaceFolder}/Invoices/SAPUI5/.env8",
      "preLaunchTask": "Build Invoices"
    },
    {
      "name": "Run Integration Test Invoices-SAPUI5",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Invoices/invoices-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsSAPUI5/test/integration/opaTests.qunit.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Invoices/SAPUI5",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Invoices/SAPUI5\"}"
      },
      "envFile": "${workspaceFolder}/Invoices/SAPUI5/.env9",
      "preLaunchTask": "Build Invoices"
    },
    {
      "name": "Start excel",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "cwd": "${workspaceFolder}/excel",
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {
        "DEBUG": "--inspect",
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/excel\"}"
      }
    },
    {
      "name": "Start excel Mock",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/excel",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "runtimeArgs": [
        "run-script",
        "start-mock"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/excel\"}"
      }
    },
    {
      "name": "Start excel Local",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/excel",
      "runtimeExecutable": "npm",
      "windows": {
        "runtimeExecutable": "npm.cmd"
      },
      "runtimeArgs": [
        "run-script",
        "start-local"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229,
      "env": {
        "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/excel\"}"
      }
    },
    {
      "name": "Run logali-Employees",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/logali/logali-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsEmployees/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/logali/Employees",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/logali/Employees\"}"
      },
      "envFile": "${workspaceFolder}/logali/Employees/.env1",
      "preLaunchTask": "Build logali"
    }
  ],
  "compounds": [],
  "version": "0.2.0"
}